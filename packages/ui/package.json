{
  "name": "@broet/ui",
  "description": "The broet Vue component library.",
  "author": "Joost Kersjes",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoostKersjes/broet.git",
    "directory": "packages/ui"
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier -uw --cache .",
    "build": "storybook build -o dist",
    "preview": "pnpm dlx serve -nL dist"
  },
  "devDependencies": {
    "@broet/storybook-theming-tailwind": "workspace:^",
    "@broet/tailwind-config": "workspace:^",
    "@rushstack/eslint-patch": "^1.5.1",
    "@storybook/addon-a11y": "^7.4.6",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-interactions": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/blocks": "^7.4.6",
    "@storybook/manager-api": "^7.4.6",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^7.4.6",
    "@storybook/vue3": "^7.4.6",
    "@storybook/vue3-vite": "^7.4.6",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.17.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.51.0",
    "eslint-plugin-storybook": "^0.6.14",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.4.6",
    "storybook-dark-mode": "^3.0.1",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.2.2",
    "vite": "^4.4.11",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.16"
